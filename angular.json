{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "manabiko-app": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {


            "outputPath": "dist/manabiko-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],


            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css"








            ],
            "scripts":


            [


              "node_modules/jquery/dist/jquery.min.js","node_modules/bootstrap/dist/js/bootstrap.js"


            ,"src/app/qgis-map/js/qgis2web_expressions.js"
            ,"src/app/qgis-map/js/leaflet.js"
            ,"src/app/qgis-map/js/L.Control.Locate.min.js"
            ,"src/app/qgis-map/js/leaflet.rotatedMarker.js"
            ,"src/app/qgis-map/js/leaflet.pattern.js"
            ,"src/app/qgis-map/js/leaflet-hash.js"
            ,"src/app/qgis-map/js/Autolinker.min.js"
            ,"src/app/qgis-map/js/rbush.min.js"
            ,"src/app/qgis-map/js/labelgun.min.js"
            ,"src/app/qgis-map/js/labels.js"
            ,"src/app/qgis-map/js/leaflet-control-geocoder.Geocoder.js"




            ]

          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "25mb",
                  "maximumError": "37mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "18kb",
                  "maximumError": "52kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "manabiko-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "manabiko-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "manabiko-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "manabiko-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "manabiko-app:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "manabiko-app",
  "cli": {
    "analytics": false
  }
}
