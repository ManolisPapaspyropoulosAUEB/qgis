{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "manabiko-app": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {


            "outputPath": "dist/manabiko-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css"

//
//            ,"src/app/qgis-map/css/leaflet.css"
//            ,"src/app/qgis-map/css/L.Control.Locate.min.css"
//            ,"src/app/qgis-map/css/qgis2web.css"
//            ,"src/appcss/leaflet-control-geocoder.Geocoder.css"




            ],
            "scripts": [ "node_modules/jquery/dist/jquery.min.js","node_modules/bootstrap/dist/js/bootstrap.js"


            ,"src/app/qgis-map/js/qgis2web_expressions.js"
            ,"src/app/qgis-map/js/leaflet.js"
            ,"src/app/qgis-map/js/L.Control.Locate.min.js"
            ,"src/app/qgis-map/js/leaflet.rotatedMarker.js"
            ,"src/app/qgis-map/js/leaflet.pattern.js"
            ,"src/app/qgis-map/js/leaflet-hash.js"
            ,"src/app/qgis-map/js/Autolinker.min.js"
            ,"src/app/qgis-map/js/rbush.min.js"
            ,"src/app/qgis-map/js/labelgun.min.js"
            ,"src/app/qgis-map/js/labels.js"
            ,"src/app/qgis-map/js/leaflet-control-geocoder.Geocoder.js"

            ,"src/app/qgis-map/data/KhostProvincedistrictsKhost_Province_UTM42n_1.js"
            ,"src/app/qgis-map/data/Khost_Province_Baak_District_OSM_roads_UTM42n_2.js"
            ,"src/app/qgis-map/data/Khost_Province_Gurbuz_District_OSM_roads_UTM42n_3.js"
            ,"src/app/qgis-map/data/Khost_Province_Jaji_Maidan_District_OSM_roads_UTM42n_4.js"
            ,"src/app/qgis-map/data/Khost_Province_Khost_District_OSM_roads_UTM42n_5.js"
            ,"src/app/qgis-map/data/Khost_Province_Manduzay_District_OSM_roads_UTM42n_6.js"
            ,"src/app/qgis-map/data/Khost_Province_Musa_Khel_District_OSM_roads_UTM42n_7.js"
            ,"src/app/qgis-map/data/Khost_Province_Nadir_Shah_Kot_District_OSM_roads_UTM42n_8.js"
            ,"src/app/qgis-map/data/Khost_Province_Qalandar_District_OSM_roads_UTM42n_9.js"
            ,"src/app/qgis-map/data/Khost_Province_Sabari_District_OSM_roads_UTM42n_10.js"
            ,"src/app/qgis-map/data/Khost_Province_Shamul_District_OSM_roads_UTM42n_11.js"
            ,"src/app/qgis-map/data/Khost_Province_Spera_District_OSM_roads_UTM42n_12.js"
            ,"src/app/qgis-map/data/Khost_Province_Tanay_District_OSM_roads_UTM42n_13.js"
            ,"src/app/qgis-map/data/Khost_Province_Tirzayee_District_OSM_roads_UTM42n_14.js"
            ,"src/app/qgis-map/data/Khost_Province_Tirzayee_District_OSM_roads_UTM42n_14.js"


            ]

          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "7mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "8kb",
                  "maximumError": "12kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "manabiko-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "manabiko-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "manabiko-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "manabiko-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "manabiko-app:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "manabiko-app",
  "cli": {
    "analytics": false
  }
}
